<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="HomeWindow" minHeight="400.0" minWidth="300.0" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gb.controllers.CloudWindowController">
    <VBox fx:id="VBoxHomeWindow" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <HBox fx:id="quickMenu" alignment="CENTER_RIGHT" maxHeight="20.0" minHeight="20.0" prefHeight="20.0">
         <children>
            <Hyperlink fx:id="logOutPanel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" maxHeight="20.0" minHeight="20.0" onAction="#Logout" prefHeight="20.0" text="Logout" textAlignment="CENTER">
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Hyperlink>
         </children>
      </HBox>
        <HBox fx:id="interactiveWindow" maxHeight="60.0" minHeight="60.0" prefHeight="60.0">
            <Pane prefHeight="60.0" prefWidth="300.0" />
         <HBox fx:id="desktopIcons" alignment="BOTTOM_LEFT" prefHeight="100.0" prefWidth="200.0" />
        </HBox>
      <TilePane fx:id="workingWindow" prefHeight="500.0" tileAlignment="TOP_LEFT" />
         <VBox fx:id="loginWindow" alignment="CENTER" prefHeight="565.0" prefWidth="530.0" VBox.vgrow="ALWAYS">
             <Label alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" prefHeight="50.0" prefWidth="160.0" text="Login" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                 <VBox.margin>
                     <Insets left="10.0" right="10.0" />
                 </VBox.margin>
                 <font>
                     <Font name="Broadway" size="18.0" />
                 </font>
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <padding>
               <Insets bottom="5.0" />
            </padding>
             </Label>
             <TextField fx:id="loginField" maxWidth="300.0" prefHeight="25.0" prefWidth="60.0">
                 <VBox.margin>
                     <Insets />
                 </VBox.margin>
             </TextField>
             <Label alignment="BOTTOM_CENTER" prefHeight="50.0" prefWidth="160.0" text="Password">
                 <VBox.margin>
                     <Insets />
                 </VBox.margin>
                 <font>
                     <Font name="Broadway" size="18.0" />
                 </font>
            <padding>
               <Insets bottom="5.0" />
            </padding>
             </Label>
             <TextField fx:id="passField" maxWidth="300.0" prefHeight="25.0" prefWidth="60.0">
                 <VBox.margin>
                     <Insets />
                 </VBox.margin>
             </TextField>
             <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#Login" prefHeight="40.0" prefWidth="210.0" text="Connect">
                 <VBox.margin>
                     <Insets top="10.0" />
                 </VBox.margin>
                 <font>
                     <Font name="Broadway" size="14.0" />
                 </font>
             </Button>
         <Hyperlink alignment="CENTER" contentDisplay="CENTER" onAction="#Registration" prefHeight="26.0" prefWidth="209.0" text="Registration" textAlignment="CENTER">
            <font>
               <Font size="14.0" />
            </font>
            <VBox.margin>
               <Insets left="160.0" right="160.0" />
            </VBox.margin>
         </Hyperlink>
         </VBox>
        <ButtonBar fx:id="buttonBar" maxHeight="0.0" minHeight="0.0" prefHeight="0.0" prefWidth="200.0" visible="false">
            <buttons>
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#UpdateList" text="Update list" />
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#RenameButton" text="Rename" />
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#AddFile" text="Add File" />
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#AddDirectory" text="Add Dir" />
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#DownloadButton" text="Download" />
                <Button layoutX="285.0" layoutY="18.0" mnemonicParsing="false" onAction="#DeleteButton" text="Delete" />
            </buttons>
           <padding>
               <Insets left="5.0" right="5.0" />
           </padding>
        </ButtonBar>
    </VBox>
</AnchorPane>
